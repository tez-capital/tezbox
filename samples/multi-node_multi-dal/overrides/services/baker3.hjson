{
  "restart_max_retries": 0,
  "user": "${USER}",
  "stop_timeout": 300,
  "executable": "octez-baker-PsRiotum",
  "restart_delay": 20,
  "environment": {
    "HOME": "${HOME}",
    "TEZOS_LOG": "* -> info"
  },
  "restart": "always",
  "args": [
    "run",
    "with",
    "local",
    "node",
    "/tezbox/context/data/node3",
    "baker3",
    "--votefile",
    "/tezbox/context/protocols/PsRiotum/vote-file.json",
    "--dal-node",
    "http://127.0.0.1:10736"
  ]
}